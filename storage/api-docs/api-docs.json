{
    "openapi": "3.0.0",
    "info": {
        "title": "Example for response examples value",
        "version": "1.0"
    },
    "paths": {
        "/api/all-services": {
            "get": {
                "tags": [
                    "Mobile App"
                ],
                "summary": "Get all services",
                "operationId": "27a06bf97bf5a8506fb36e9b6b625d9a",
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Service"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/register": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Register a new mobile user and issue an OTP for verification.",
                "operationId": "ca7c0cb6772de9bffbb8c896980cebc8",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/RegisterRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "User registered. Please verify OTP sent to your phone."
                    },
                    "400": {
                        "description": "Invalid request"
                    }
                }
            }
        },
        "/api/verify-otp": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Verify OTP (One Time Password) for a newly registered mobile user.",
                "operationId": "7ffdcc2529f1d1bbbbac715c91b1c809",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/VerifyOtpRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Phone number successfully verified or already verified."
                    },
                    "400": {
                        "description": "Invalid request"
                    }
                }
            }
        },
        "/api/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Login a mobile user and issue an access token.",
                "operationId": "c373bc4cffa4a56fc64e3979b8ee7b96",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/LoginRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Access token issued successfully."
                    },
                    "400": {
                        "description": "Invalid request"
                    }
                }
            }
        },
        "/api/logout": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Logout a mobile user and revoke the access token.",
                "operationId": "5ff2dc8b48bff52908d3821a0ecc9b3e",
                "responses": {
                    "200": {
                        "description": "Successfully logged out."
                    }
                }
            }
        },
        "/api/logout-other-devices": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Logout a mobile user from other devices and revoke the access token.",
                "operationId": "656037f5c51140ff318938bb49f56b7a",
                "responses": {
                    "200": {
                        "description": "Successfully logged out from other devices."
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "RegisterRequest": {
                "title": "Register Request",
                "description": "Request data for registering a new user",
                "required": [
                    "name",
                    "email",
                    "password",
                    "password_confirmation"
                ],
                "properties": {
                    "phone_number": {
                        "type": "string"
                    },
                    "password": {
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "VerifyOtpRequest": {
                "title": "Verify OTP Request",
                "description": "Request data for verifying OTP",
                "required": [
                    "phone_number",
                    "otp_code"
                ],
                "properties": {
                    "phone_number": {
                        "type": "string"
                    },
                    "otp_code": {
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "LoginRequest": {
                "title": "Login Request",
                "description": "Request data for logging in a user",
                "required": [
                    "phone_number",
                    "password"
                ],
                "properties": {
                    "phone_number": {
                        "type": "string"
                    },
                    "password": {
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "Service": {
                "title": "Service",
                "description": "Service data",
                "required": [
                    "name",
                    "description",
                    "price",
                    "image"
                ],
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    },
                    "price": {
                        "type": "number"
                    },
                    "image": {
                        "type": "string"
                    }
                },
                "type": "object"
            }
        }
    },
    "tags": [
        {
            "name": "Mobile App",
            "description": "Mobile App"
        },
        {
            "name": "Auth",
            "description": "Auth"
        }
    ]
}